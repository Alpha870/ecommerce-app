{
  "version": 3,
  "builds": [
    {
      "src": "/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/server/apis/products" },
  ]
}